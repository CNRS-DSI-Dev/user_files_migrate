<?xml version="1.0" encoding="UTF-8" ?>
<database>
    <name>*dbname*</name>
    <create>true</create>
    <overwrite>false</overwrite>
    <charset>utf8</charset>
    <table>
        <name>*dbprefix*user_files_migrate</name>
        <declaration>
            <field>
                <name>id</name>
                <type>integer</type>
                <default>0</default>
                <notnull>true</notnull>
                <autoincrement>1</autoincrement>
            </field>
            <field>
                <name>requester_uid</name>
                <type>text</type>
                <default></default>
                <notnull>true</notnull>
                <length>64</length>
            </field>
            <field>
                <name>recipient_uid</name>
                <type>text</type>
                <default></default>
                <notnull>true</notnull>
                <length>64</length>
            </field>
            <field>
                <name>date_request</name>
                <type>timestamp</type>
                <notnull>true</notnull>
            </field>
            <field>
                <name>date_end</name>
                <type>timestamp</type>
            </field>
            <field>
                <name>closed</name>
                <type>boolean</type>
                <default>0</default>
            </field>
        </declaration>
    </table>
</database>
